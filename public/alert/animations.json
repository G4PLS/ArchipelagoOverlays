{
    "fade-in": {
        "duration": 2500,
        "timing": "linear",
        "iterations": 1,
        "keyframes": {
            "0%": {
                "opacity": 0
            },
            "100%": {
                "opacity": 1
            }
        }
    },
    "fade-out": {
        "duration": 2500,
        "timing": "linear",
        "iterations": 1,
        "keyframes": {
            "0%": {
                "opacity": 1
            },
            "100%": {
                "opacity": 0
            }
        }
    },
    "fade-in-fade-out": {
        "duration": 2500,
        "timing": "linear",
        "iterations": 1,
        "keyframes": {
            "0%": {
                "opacity": 0
            },
            "50%": {
                "opacity": 1
            },
            "100%": {
                "opacity": 0
            }
        }
    }
}